{
  "name": "unit-test-generator-for-cpp",
  "displayName": "unit-test-generator-for-cpp",
  "description": "Generate unit tests for C++ code using OpenAI's GPT-3.",
  "version": "0.0.1",
  "publisher": "publisher",
  "engines": {
    "vscode": "^1.90.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unit-test-generator-for-cpp.createUnitTest",
        "title": "Generate Unit Test"
      },
      {
        "command": "unit-test-generator-for-cpp.createIntegrationTest",
        "title": "Generate Integration Test"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "unit-test-generator-for-cpp.createUnitTest",
          "when": "editorTextFocus",
          "group": "navigation"
        },
        {
          "command": "unit-test-generator-for-cpp.createIntegrationTest",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/vscode": "^1.60.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "openai": "^4.49.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  }
}
